@artigo{Ioannidis2005Faux,
    doi = {10.1371/journal.pmed.0020124},
    autor = {Ioannidis, John P. A.},
    periodico = {PLOS Medicine},
    Éditeur = {Public Library of Science},
    título = {Why Most Published Research Findings Are False},
    ano = {2005},
    moi·s = {08},
    volume = {2},
    url = {https://doi.org/10.1371/journal.pmed.0020124},
    páginas = {null},
    abstract = {Resumo Há uma preocupação crescente com o fato de que a maioria das descobertas de pesquisas publicadas atuais são falsas. A probabilidade de uma reivindicação de investigação ser verdadeira pode depender do poder e do viés do estudo, do número de outros estudos sobre a mesma questão, e, o que é importante, a relação de verdade em relação a nenhuma relação entre as relações sondadas em cada campo científico. Neste contexto, é menos provável que uma investigação seja feita quando os estudos realizados num domínio são menores; quando tamanhos de efeito são menores; quando há um maior número e uma menor pré-seleção de relações testadas; onde há uma maior flexibilidade nos desenhos, definições, resultados e modos analíticos; quando há maior interesse e preconceito financeiros; e quando há mais equipas envolvidas num domínio científico em busca de significado estatístico. As simulações mostram que na maioria dos projetos de estudo e configurações, é mais provável que uma pesquisa seja falsa do que verdadeira. Além disso, para muitos domínios científicos actuais, as alegadas descobertas da investigação podem muitas vezes ser medidas correctas do preconceito prevalecente. Nesta redaccaçaça<unk> o, discuto as implicações destes problemas para a conducça<unk> o e interpretaçaça<unk> o da investigac<unk> o.},
    numéro = {8},
}

